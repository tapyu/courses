-- Teste inicial usando porta lógica
-- a, b; entry
-- y: saída
-- FUNÇÃO NAND
--
--
--02/18


-- declaração de bibliotecas e pacotes

ENTITY lab1 IS
	
	PORT (a,b	:	IN		BIT;
			y		:	OUT	BIT);

END lab1;

ARCHITECTURE hardware OF lab1 IS

-- declaração de variável

BEGIN
	
	y<= a NAND b;


END hardware